{
  "name": "builder-bundle",
  "version": "0.0.6",
  "description": "Headless UI component library for nouns builder ecosystem.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "pack-local": "npm pack --pack-destination ~"
  },
  "keywords": [
    "nouns",
    "ethereum",
    "web3",
    "react"
  ],
  "author": "0xmcg (frog)",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.0.26",
    "react": "^18.2.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "ethers": "^5.7.2",
    "graphql": "^16.6.0",
    "graphql-request": "^5.1.0",
    "graphql-tag": "^2.12.6",
    "swr": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcgingras/builder-bundle-headless.git"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ]
}
